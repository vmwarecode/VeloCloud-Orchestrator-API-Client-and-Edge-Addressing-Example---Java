    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "net.velocloud.ConfigureEdgeAddressing",
            "projectName": "vco_client",
            "env": {
                "VC_HOSTNAME": "<vco_url>",
                "VC_USERNAME": "<username>",
                "VC_PASSWORD": "<password>",
                "VC_ENTERPRISE_ID": 1,
                "VC_EDGE_ID": 1,
                "VERIFY_SSL": false
            }
        }
    ]
}